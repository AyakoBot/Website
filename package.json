{
	"name": "@ayako/website",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "rm -rf ./.svelte-kit && vite dev --port 5173",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "vite preview",
		"start": "rm -r ./.svelte-kit && pnpm build && pnpm preview --port 5173"
	},
	"dependencies": {
		"@iconify-json/tabler": "^1.2.13",
		"@sveltejs/adapter-cloudflare": "^5.0.0",
		"@unocss/preset-icons": "^0.65.3",
		"@unocss/reset": "^0.65.3",
		"clsx": "^2.1.1",
		"discord-api-types": "0.37.114",
		"dotenv": "^16.4.7",
		"js-cookie": "^3.0.5",
		"tailwind-merge": "^2.6.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.15.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/eslint": "^9.6.1",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^22.10.2",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.19.0",
		"@unocss/preset-rem-to-px": "0.65.3",
		"@unocss/svelte-scoped": "^0.65.3",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "5.16.0",
		"typescript": "^5.7.2",
		"unocss": "^0.65.3",
		"unocss-preset-animations": "^1.1.0",
		"unocss-preset-shadcn": "^0.3.1",
		"vite": "^6.0.6"
	},
	"packageManager": "pnpm@9.10.0",
	"engines": {
		"node": "^22.3.0",
		"pnpm": "^9.5.0"
	}
}
